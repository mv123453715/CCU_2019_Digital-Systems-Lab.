    %assign/v0 v0101A880_0, 0, 8;
    %load/v 8, v0101A8D8_0, 6;
    %mov 14, 0, 26;
    %addi 8, 1, 32;
    %ix/load 0, 6, 0;
    %assign/v0 v0101A8D8_0, 0, 8;
    %load/v 8, v01050DE8_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_5.2, 4;
    %load/v 8, v010512B8_0, 16;
    %load/v 24, v0101A880_0, 16;
    %cmp/u 8, 24, 16;
    %inv 4, 1;
    %jmp/0xz  T_5.4, 4;
