    %load/v 8, v0101AB10_0, 8;
    %mov 16, 0, 8;
    %ix/load 0, 16, 0;
    %assign/v0 v01018568_0, 0, 8;
    %load/v 8, v0101AB68_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0101A310_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %load/v 8, v01018510_0, 6;
   %cmpi/u 8, 8, 6;
    %or 5, 4, 1;
    %jmp/0xz  T_1.4, 5;
    %load/v 8, v0101A310_0, 1; Only need 1 of 8 bits
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_1.6, 8;
    %load/v 8, v01018568_0, 16;
    %load/v 24, v0101A368_0, 16;
    %add 8, 24, 16;
    %ix/load 0, 16, 0;
    %assign/v0 v0101A368_0, 0, 8;
T_1.6 ;
    %load/v 8, v01018568_0, 16;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftl/i0  8, 16;
    %ix/load 0, 16, 0;
    %assign/v0 v01018568_0, 0, 8;
    %load/v 8, v0101A310_0, 8;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftr/i0  8, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0101A310_0, 0, 8;
    %jmp T_1.5;
T_1.4 ;
    %load/v 8, v0101A368_0, 16;
